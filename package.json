{
  "name": "fantasy-baseball-monorepo",
  "version": "1.0.0",
  "description": "Fantasy Baseball Research MVP - Monorepo",
  "scripts": {
    "dev:backend": "cd backend && ./start.sh",
    "dev:frontend": "cd frontend && python3 -m http.server 8001",
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "install:backend": "cd backend && ./venv/bin/pip install -r requirements.txt",
    "test": "cd backend && ./venv/bin/pytest"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "workspaces": [],
  "keywords": ["fantasy", "baseball", "api", "fastapi"],
  "author": "Steve <steveclawdbot@gmail.com>",
  "license": "MIT"
}
